LIBRARY ieee;
USE ieee.std_logic_1164.all;
USE ieee.numeric_std.all;

LIBRARY MP2;
USE MP2.LC3b_types.all;

ENTITY WordMux4 IS
   PORT( 
      A   : IN     LC3b_word;
      B   : IN     LC3b_word;
      C   : IN     LC3b_word;
      D   : IN     LC3b_word;
      Sel : IN     std_logic_vector (1 DOWNTO 0);
      F   : OUT    LC3b_word
   );

-- Declarations

END WordMux4 ;

-- hds interface_end
ARCHITECTURE untitled OF WordMux4 IS
BEGIN
	PROCESS (A, B, C, D, Sel)
		variable state : LC3b_word;
	BEGIN
		case Sel is
			when "00" =>
				state := A;
			when "01" =>
				state := B;
			when "10" =>
				state := C;
			when "11" =>
				state := D;
			when others =>
				state := (OTHERS => 'X');
		end case;
		F <= state after delay_MUX4;
	END PROCESS;




END untitled;
